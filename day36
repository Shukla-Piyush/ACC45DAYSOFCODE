#include<iostream>
using namespace std;
class complex{
    int x,y;
    public:
    void setid(int a,int b){
        x=a;
        y=b;
    }
    void setdata(complex o1,complex o2){
        x=o1.x+o2.x;
        y=o1.y+o2.y;
    }
    void print(){
        cout<<x<<"+i"<<y<<endl;
    }  
};
    int main(){
        complex c1,c2;
        c1.setid(1,3);
        c1.print();
        c2.setid(4,5);
        c2.print();
        return 0;

    }
