#include <iostream>
using namespace std;

int main() {
    int t;
   int  a=0;
  int   b=0;
    cin>>t;
    int n[t];
    for(int i=0;i<t;i++){
        cin>>n[i];
         if(n[i]%2==0){
        a++;}
        else
        b++;
    }
    if(a>b){
        cout<<"READY FOR BATTLE"<<endl;
    }
    else
    cout<<"NOT READY "<<endl;
	// your code goes here
	returnÂ 0;
}
